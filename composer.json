{
    "name": "dnstaxi/joe",
    "description": "DNS Taxi - Joe: A utility tool to build and manage DNS zone files on remote DNS servers",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "symfony/console": "^6.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "humbug/box": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": ["bin/console"]
}
